        "# 台形の辺と高さを定義\n",
        "upper_base = 10 # 上底 (cm)\n",
        "lower_base = 20 # 下底 (cm)\n",
        "height = 5      # 高さ (cm)\n",
        "\n",
        "# 面積を計算\n",
        "area =  (upper_base+lower_base)*height/2\n",
        "\n",
        "# 結果を出力\n",
        "print(f\"{area}cm²\")"
      ]
